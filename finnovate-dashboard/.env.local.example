# LOCAL TESTING CONFIGURATION
# Use this for testing with local AI bot server
# Rename to .env when testing locally

# Local AI Bot Server
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=10000

# App Configuration
REACT_APP_APP_NAME=Finnovate AI Dashboard (Local Test)
REACT_APP_VERSION=1.0.0-dev
REACT_APP_AWS_REGION=ap-southeast-1

# Feature Flags for Local Testing
REACT_APP_ENABLE_AI_CHATBOT=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_DEBUG=true

# Disable Authentication for Local Testing
REACT_APP_DISABLE_AUTH=true
REACT_APP_MOCK_USER=true